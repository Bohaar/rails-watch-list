<div class='container'>
<div class= 'box'>
  <h1>Lists</h1>
    <ul class="list-group mb-3">
        <% @lists.each do |list| %>
           <li> <%= link_to list.name, list_path(list.id) %>
          </li>
        <% end %>
        <%= link_to "Add a new List", new_list_path, class: 'btn btn-primary'%>
</div>
</div>
